    <PackageReadmeFile>Readme.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Seowsoft.png" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\SeowCron\Src\SeowCronLib\SeowCronLib\Readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\..\..\SeowCron\Src\SeowCronLib\SeowCronLib\Seowsoft.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

